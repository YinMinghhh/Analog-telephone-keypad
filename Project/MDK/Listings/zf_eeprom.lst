  91              IAP_ADDRH = addr >> 8;  //设置 IAP 高地址
  92              IAP_TRIG = 0x5a;    //写触发命令(0x5a)
  93              IAP_TRIG = 0xa5;    //写触发命令(0xa5)  
  94              *buf++ = IAP_DATA;    //读 IAP 数据
  95              addr++;
  96              _nop_(); 
  97            }
  98          }
